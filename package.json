{
  "name": "pasiopadelclub",
  "description": "Site web du Pasio Padel Club - Réservation de terrains de padel à Bayonne",
  "author": "Pasio Padel Club <pasio.padel.club@gmail.com>",
  "license": "UNLICENSED",
  "keywords": [
    "padel",
    "bayonne",
    "reservation",
    "sport"
  ],
  "private": true,
  "type": "module",
  "packageManager": "npm@11.8.0",
  "engines": {
    "node": "24.4.1"
  },
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node .output/server/index.mjs",
    "test": "vitest run",
    "lint": "npx tsc && eslint",
    "lint:fix": "npm run lint -- --fix",
    "deps": "npx taze@latest -Ilw",
    "deps:major": "npx taze@latest major -Ilw",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "npx tsx src/db/seed.ts",
    "db:seed:credit-packs": "npx tsx src/db/seed-credit-packs.ts",
    "email:dev": "email dev --dir src/emails --port 3001",
    "email:export": "email export --dir src/emails --outDir dist/emails",
    "cron:reminder": "vite-node --config vite.config.cron.ts cron/reminder.ts",
    "cron:retention": "vite-node --config vite.config.cron.ts cron/data-retention.ts",
    "cron:unverified-cleanup": "vite-node --config vite.config.cron.ts cron/unverified-cleanup.ts",
    "cron:verification-reminder": "vite-node --config vite.config.cron.ts cron/verification-reminder.ts",
    "clean": "rm -rf .output dist node_modules/.cache"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.6",
    "@stripe/react-stripe-js": "^5.5.0",
    "@stripe/stripe-js": "^8.6.4",
    "@t3-oss/env-core": "^0.13.10",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-query": "5.90.20",
    "@tanstack/react-router": "^1.157.16",
    "@tanstack/react-router-ssr-query": "^1.157.16",
    "@tanstack/react-start": "^1.157.16",
    "@tanstack/router-plugin": "^1.157.16",
    "better-auth": "^1.4.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "embla-carousel-react": "^8.6.0",
    "libphonenumber-js": "^1.12.35",
    "lucide-react": "^0.563.0",
    "motion": "^12.29.2",
    "nitro": "^3.0.1-alpha.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "resend": "^6.9.1",
    "satori": "^0.19.1",
    "schema-dts": "^1.1.5",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@react-email/preview-server": "^5.2.5",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/eslint-config": "^0.3.4",
    "@tanstack/react-devtools": "^0.9.3",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.157.16",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@viclafouch/eslint-config-viclafouch": "4.22.1-beta.7",
    "@vitejs/plugin-react": "^5.1.2",
    "chokidar": "^5.0.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "husky": "^9.1.7",
    "lru-cache": "^11.2.5",
    "prettier": "^3.8.1",
    "react-email": "^5.2.5",
    "readdirp": "^5.0.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-node": "^5.3.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  }
}
